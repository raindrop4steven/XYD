{
    "title": "住建局公文处理单",
    "header": [
        {
            "key": "公文标题",
            "value": "#valueof($.DocumentTitle)"
        },
        {
            "key": "来文单位",
            "value": "#valueof($.ReceivedDocumentUnit)"
        },
        {
            "key": "收文日期",
            "value": "#valueof($.ClosedOrHairTime)"
        },
        {
            "key": "来文文号",
            "value": "#valueof($.DocumentNumber)"
        },
        {
            "key": "备注",
            "value": "#valueof($.memo)"
        }
    ],
    "sections": [
        {
            "multiple": false,
            "key": "党政办拟办",
            "data": [
                {
                    "user": "#valueof($.nibanUser)",
                    "text": "#valueof($.nibanOpinion)",
                    "time": "#valueof($.nibanTime)"
                }
            ]
        },
        {
            "multiple": true,
            "key": "领导批示",
            "data": "#customfunction(DeptOA, DeptOA.Common.TransformUtil.TransformArray, #valueof($.leaderOpinion))"
        },
        {
            "multiple": true,
            "key": "部门承办",
            "data": "#customfunction(DeptOA, DeptOA.Common.TransformUtil.TransformArray, #valueof($.deptOpinion))"
        }
    ]
}
